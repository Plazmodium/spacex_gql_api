{
    "version": 4,
    "builds":[
        {
            "src": "build/index.ts",
            "use":"@vercel/node",
            "config": {
                "includeFiles": ["build/**"]
            }
        }
    ],
    "routes":[
        {
            "src": "/(.*)",
            "dest":"build/index.ts"
        }
    ]
}